#!/bin/bash

ls -l /
id -un
echo "run"
if [ $2 = "wine" ] || [ -z "$2"] ; then
	export LC_ALL='zh_CN.UTF-8' LANG='zh_CN.UTF-8' LANGUAGE='zh_CN:zh:en_US:en' 
fi
export TZ='Asia/Shanghai'
apache2
tightvncserver :1
setsid /var/www/html/noVNC-master/utils/launch.sh --vnc localhost:5901 &
exec /usr/sbin/sshd -D
exit 0